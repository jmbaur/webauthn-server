{
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment"
  },
  "tasks": {
    "install": "deno cache --reload --lock=lock.json main.tsx",
    "upgrade": "deno cache --lock=lock.json --lock-write main.tsx",
    "build": "packup build index.html"
  }
}
